{
	"id": "user",
	"data": {
		"input": ["auth", "name", "email"],
		"list": ["name", "email", "auth.username", "auth.email"],
		"load": ["auth", "role", "name", "email", "bro", "abc", "def.username", "testArRf.thBro", "testArObRf.one", "testArObRf.two"],
		"save": ["auth", "role", "bro", "name", "email", "abc", "def", "testArRf", "testArObRf", "testObRf"],
		"schema": {
			"id": "/user",
			"type": "object",
			"properties": {
				"auth": {
					"title": "Account",
					"$ref": "/auth/id"
				},
				"role": {
					"title": "Roles",
					"type": "array",
					"items": {"type": "string", "maxLength": 50, "required": true }
				},
				"bro": {
					"title": "Brother",
					"$ref": "/user/id"
				},
				"name": {
					"title": "Name",
					"type": "string",
					"minLength": 0,
					"maxLength": 255,
					"required": true
				},
				"email": {
					"title": "Email Address",
					"type": "string",
					"minLength": 16,
					"maxLength": 255
				},
				"phone": {
					"title": "Phone Number",
					"type": "string",
					"minLength": 0,
					"maxLength": 25
				},
				"abc": {
					"title": "ABC",
					"$ref": "/auth/id"
				},
				"def": {
					"title": "DEF",
					"$ref": "/auth/id"
				},
				"testArRf": {
					"type": "array",
					"items": {
						"$ref": "/auth/id"
					}
				},
				"testArObRf": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"one": { "type": "string" },
							"two": { "$ref": "/auth/id" },
							"three": { "type": "number" }
						}
					}
				},
				"testObRf": {
					"type": "object",
					"properties": {
						"rfz": { "title": "RFZ", "$ref": "/auth/id" },
						"thing": { "type": "integer" }
					}
				}
			}
		}
	},
	"routes": {
		"/": {
			"callback": "list",
			"methods": { "GET": true, "POST": true }
		},
		"/:id": {
			"callback": "profile",
			"methods": { "GET": true, "PUT": true },
			"form": [
				{ "key": "name", "placeholder": "Full Name" },
				{ "key": "email", "placeholder": "Email address" },
				{ "key": "phone", "placeholder": "Phone number" },
				{ "type": "section", "htmlClass": "btn-group inline-btn-group", "items": [
						{ "type": "submit", "style": "btn-primary", "title": "Save" },
						{ "type": "button", "style": "btn-warning", "title": "Reset", "onClick": "reset()" }
				]}
			]
		}
	},
	"screens": {
		"*": {
			"nav": [
				{ "type": "link",
					"path": "/admin/user",
					"title": "User Admin",
					"access": ["admin"]
				}
			]

		},
		"/user*": {
			"nav": [
				{ "type": "link",
					"path": "/user",
					"title": "View Profile",
					"access": ["user"]
				},
				{ "type": "link",
					"path": "/user/edit",
					"title": "Edit Profile",
					"access": ["user"]
				}
			]
		},
		"/user/:user_id<\\d+>?/:action<view|edit>?": {
			"page": [
				{ "type": "widget",
					"content": "tn-user",
					"args": {"id": ":user_id?!auth_id", "state": ":action?view"},
					"access": ["user"]
				}
			]
		},
		"/admin": {
			"page": [
				{ "type": "widget",
					"content": "tn-user-admin",
					"access": ["admin"]
				}
			]
		},
		"/admin/user*": {
			"tabs": [
				{ "type": "widget",
					"content": "tn-tabs",
					"args": {"actions": {"/admin/user": "Manage's Users", "/admin/user/add": "Add User"}},
					"access": ["admin"]
				}
			],
			"page": [
				{ "type": "widget",
					"content": "tn-user-list",
					"access": ["admin"]
				}
			]
		},
		"/admin/user/:user_id<\\d+>": {
			"page": [
				{ "type": "widget",
					"content": "tn-user",
					"args": {"id": ":user_id"},
					"access": ["admin"]
				}
			]
		}
	}
}